#!/bin/bash
#
#

read -p "Sisestage suvaline number: " number #loeb sisestust
i=2
while [ $i -lt $number ] #ts端kkli algus
do # alustab do kasuga
if [ `expr $number % $i` -eq 0 ] #alustab if kasuga
then 
echo "$number - ei ole algarv" #端tleb kas on v ei
echo "Kuna seda saab jagada $i"
exit
fi
i=`expr $i + 1` #ts端kkli arvu lisamine
done
echo "$number - on algarv " #端tleb, et kas on algarv v ei
